{
  "expo": {
    "name": "Crux",
    "slug": "crux",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "crux",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1117"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D1117"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.crux.app"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
